<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.english.englishproject">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.USE_FINGERPRINT" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_round"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".WelcomeActivity" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".RegisterActivity" />
        <activity android:name=".HomeActivity" />
        <activity android:name=".VideoBackgroundActivity" />
        <activity android:name=".ProfileActivity" />
        <activity android:name=".CourseActivity" />
        <activity
            android:name=".VideoFullscreenActivity"
            android:screenOrientation="landscape" />
        <activity
            android:name=".RecyclerView_Video_Activity"
            android:screenOrientation="portrait" />
        <activity android:name=".GameActivity" />
        <activity
            android:name=".PlanetShooterGameActivity"
            android:configChanges="orientation|keyboardHidden"
            android:screenOrientation="portrait" />
        <activity
            android:name=".StartGame"
            android:configChanges="orientation|keyboardHidden"
            android:screenOrientation="portrait" />
        <activity
            android:name=".GameOverActivity"
            android:configChanges="orientation|keyboardHidden"
            android:screenOrientation="portrait" />
        <activity android:name=".WordMattersActivity" />
        <activity android:name=".BossWordActivity" />
        <activity android:name=".activities.BooksActivity" />
        <activity android:name=".activities.MyBooksActivity" />
        <activity android:name=".AboutActivity"></activity>
    </application>

</manifest>